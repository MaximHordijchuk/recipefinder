<h1>Recipes#index</h1>
<p>Find me in app/views/recipes/index.html.erb</p>
<table border="1">
  <tr>
    <th>Image</th>
    <th>Title</th>
    <th>Rate</th>
  </tr>
  <% @recipes.each do |recipe| %>
    <tr>
      <td><%= link_to image_tag(recipe['image_url'], width: 100, height: 100), recipe['f2f_url'], :target => '_blank' %></td>
      <td><%= link_to raw(recipe['title']), recipe['f2f_url'], :target => '_blank' %></td>
      <td><%= recipe['social_rank'] %></td>
    </tr>
  <% end %>
</table>